<script>
$(function() {
	var map = new Map("/images/<%= @map.image_filename %>", {gridX: <%= @map.width %>, gridY:<%= @map.height %>, xOffset:<%= @map.x_offset %>, yOffset:<%= @map.y_offset %>});

	<% @map.entities.each do |entity| %>
		new MapObject("<%= entity.name %>", map, "/images/<%= entity.image_filename %>", {left:<%= entity.pos_x %>,top:<%= entity.pos_y %>,size:<%= entity.size %>,locked:<%= entity.locked.inspect %>,deadzone:<%= entity.deadzone.inspect %>});
		<%= %> 
	<% end %>
});
</script>
<%= render :partial => '/entities/form' %>