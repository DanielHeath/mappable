<% ename = entity.name.gsub(' ','_') %>
entities["<%=ename%>"] = new MapObject("<%= entity.name %>", 
	map, 
	"<%= entity.image_url %>", 
	{
		left:<%= entity.pos_x %>,
		top:<%= entity.pos_y %>,
		size:<%= entity.size %>,
		locked:<%= (entity.is_controllable_by(current_user) ? entity.locked : true).inspect %>,
		deadzone:<%= entity.deadzone.inspect %>,
		id: <%= entity.id %>,
		charInfoPanelID: "<%= infoPanelID(entity) %>"
	}
);

